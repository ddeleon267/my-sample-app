<h2>Edit a Post!</h2>
<br>
<h2>Add a Post!</h2>
<br>
<form method="post" action="/posts/<%=@post.id%>">
    <input type="hidden" id="hidden" name="_method" value="patch">
        <br>
        <label>Title:</label>
        <input type="text" name="title" value="<%=@post.title %>">
        <br>
        <textarea name="body"><%= @post.body %></textarea>
        <br>
        <input type="submit">
</form>